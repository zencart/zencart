{
  "name": "zcwilt/rest-api",
  "type": "library",
  "license": "MIT",
  "description": "An API framework for Laravel",
  "keywords": ["rest", "api", "laravel"],
  "authors": [
    {
      "name": "Ian C Wilson"
    }
  ],
  "autoload": {
    "psr-4": {
      "Zcwilt\\Api\\": "src/"
    }
  },
  "require": {
    "illuminate/database": "^5.4|^7.0",
    "illuminate/auth": "^5.4|^7.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.0",
    "phpunit/phpunit": "^7.0",
    "codedungeon/phpunit-result-printer": "^0.19.12",
    "nunomaduro/collision": "^2.0",
    "mockery/mockery": "^1.1"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
